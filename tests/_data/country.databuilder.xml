<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="Country">
        <property name="iso2Code" dataBuilderRule="=DE"/>
        <property name="iso3Code" dataBuilderRule="=DEU"/>
        <property name="name" dataBuilderRule="=Germany"/>
    </transfer>

    <transfer name="Region">
        <property name="iso2Code" dataBuilderRule="=DE"/>
        <property name="name" dataBuilderRule="=word(1)"/>
    </transfer>

</transfers>
